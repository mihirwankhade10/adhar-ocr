<!-- views/index.ejs -->
<!DOCTYPE html>
<html>
  <head>
    <title>Image Uploader</title>
  </head>
  <body>
    <h1>Image Uploader</h1>
    <form action="/upload" method="POST" enctype="multipart/form-data">
      <input type="file" name="image" accept="image/*" />
      <button type="submit">Upload</button>
    </form>
  </body>
</html>
